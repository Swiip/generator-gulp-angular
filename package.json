    "bower": "~1.3.3",
    "chai": "~1.9.1",
    "chai-as-promised": "~4.1.1",
    "colors": "~0.6.2",
    "coveralls": "~2.11.0",
    "cross-spawn": "~0.2.3",
    "gulp-less": "^1.3.3",
    "gulp-sass": "^0.7.3",
    "istanbul": "~0.3.0",
    "lodash": "~2.4.1",
    "mocha": "~1.21.0",
